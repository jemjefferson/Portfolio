@model ProjectViewModel;

<div class="row">
@foreach (Project p in Model.WebApps)
{
    <div class="column">
            <a href="@p.Link" target="_blank">
            <div class="card">
                <h3>@p.Name</h3>
                <img src="@p.Image" id="cardPhoto"/>
                <p>@p.Description</p>
            </div>
            </a>
            <div class="desc">
                <p class="descText">@p.FrameWork</p>
            </div>
        </div>
}
    <div class="column">
        <a asp-action="Index">
            <div class="card">
            <h3>Portfolio</h3>
            <img src="../images/jordan.jpg" id="cardPhoto"/>
            <p>My personal portfolio website.</p>
        </div>
        </a>
        <div class="desc">
            <p class="descText">ASP.NET MVC</p>
        </div>
    </div>
    <div class="space"></div>
</div>
<div class="buttonContainer">
    <a href="https://github.com/jemjefferson" target="_blank"><button id="projectsButton">Source Code</button></a>
</div>